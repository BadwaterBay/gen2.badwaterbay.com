{
  "name": "gen2.badwaterbay.com",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "https://github.com/BadwaterBay/gen2.badwaterbay.com.git",
  "author": "Badwater Bay",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ci": "yarn --frozen-lockfile",
    "format": "prettier './**/*.{js,jsx,ts,tsx,json,html,md,css,scss}' --write",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --cache --fix",
    "clean": "rm -rf build",
    "build": "yarn clean && react-scripts build",
    "test": "react-scripts test",
    "serve:prod": "serve -s build",
    "start:dev": "NODE_ENV=development && yarn ci && react-scripts start",
    "start:prod": "NODE_ENV=production && run-s ci build serve:prod",
    "start": "yarn start:dev",
    "eject": "react-scripts eject",
    "refresh": "rm -rf node_modules && yarn ci"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,html,md,css,scss}": [
      "prettier --write"
    ],
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@dongskyler/helpers.js": "^0.3.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/dom": "^7.21.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-color": "^2.18.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-router": "~4.3.0",
    "react-router-dom": "^5.2.0",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "3.4.3",
    "sass": "^1.26.10",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "~2.34.0",
    "@typescript-eslint/parser": "~2.34.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "eslint": "~6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.4",
    "eslint-plugin-react-hooks": "~2.5.1",
    "husky": "^4.2.5",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "serve": "^11.3.2",
    "typescript": "^3.9.7"
  }
}

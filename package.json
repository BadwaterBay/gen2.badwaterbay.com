{
  "name": "gen2.badwaterbay.com",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "https://github.com/BadwaterBay/gen2.badwaterbay.com.git",
  "author": "Badwater Bay",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "format": "prettier './**/*.{js,jsx,ts,tsx,json,html,md,css,scss,yaml,yml}' --write",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --cache --fix",
    "clean": "rimraf build",
    "build": "yarn clean && react-scripts build",
    "test": "react-scripts test",
    "serve:prod": "serve -s build",
    "start:dev": "NODE_ENV=development && react-scripts start",
    "start:prod": "NODE_ENV=production && yarn build && yarn serve:prod",
    "start": "yarn start:dev",
    "eject": "react-scripts eject",
    "refresh": "rimraf node_modules && yarn --frozen-lockfile"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,html,md,css,scss,yaml,yml}": [
      "prettier --write"
    ],
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@dongskyler/helpers.js": "^0.3.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-color": "^2.18.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-router": "~4.3.0",
    "react-router-dom": "^5.2.0",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "3.4.3",
    "sass": "^1.32.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.1.6",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "~2.34.0",
    "@typescript-eslint/parser": "~2.34.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "eslint": "~6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "~2.5.1",
    "husky": "^4.3.7",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "serve": "^11.3.2",
    "typescript": "^4.1.3"
  }
}
